generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model participantes {
  id         String   @id @default(auto()) @map("_id") @db.ObjectId
  github_url String
  name       String
  status     String
  created    DateTime
  updated    DateTime?
}

model teste {
  id   String @id @default(auto()) @map("_id") @db.ObjectId
  name String
}
